function setup() {
  createCanvas(400,400);
  box = createSprite (200,200,20,20)
  
}

function draw() 
background(white);
  if (keyIsDown (RIGHT_ARROW)) {
  backround(red)
  }

   if (keyIsDown (LEFT_ARROW)) {
  backround(blue)
  }

   if (keyIsDown (UP_ARROW)) {
  backround(pink)
  }

   if (keyIsDown (RIGHT_ARROW)) {
  backround(yellow)
  }

   if (keyIsDown (DOWN_ARROW)) {
  backround(green)
  }
drawSprites ();




